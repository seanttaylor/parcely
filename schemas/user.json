{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://schemas.parcely.com/user.json",
    "type": "object",
    "title": "The User schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "id": "fee7b553-f3d1-4a6f-82ae-7e37e1ed697c",
            "firstName": "Nick",
            "lastName": "Fury",
            "phoneNumber": "2125552424",
            "emailAddress": "nfury@shield.gov",
            "password": "superSecretPassword",
            "createdDate": "2021-02-24T17:45:36.230152",
            "lastModified": "2021-01-24T17:45:36.230152"
        }
    ],
    "required": [
        "id",
        "firstName",
        "lastName",
        "phoneNumber",
        "emailAddress",
        "password",
        "createdDate",
        "lastModified"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "string",
            "title": "The id schema",
            "description": "The user uuid.",
            "default": "",
            "examples": [
                "fee7b553-f3d1-4a6f-82ae-7e37e1ed697c"
            ]
        },
        "firstName": {
            "$id": "#/properties/firstName",
            "type": "string",
            "title": "The firstName schema",
            "description": "The user first name.",
            "default": "",
            "examples": [
                "Nick"
            ]
        },
        "lastName": {
            "$id": "#/properties/lastName",
            "type": "string",
            "title": "The lastName schema",
            "description": "The user last name.",
            "default": "",
            "examples": [
                "Fury"
            ]
        },
        "phoneNumber": {
            "$id": "#/properties/phoneNumber",
            "type": "string",
            "title": "The phoneNumber schema",
            "description": "The user phone number.",
            "default": "",
            "examples": [
                "2125552424"
            ]
        },
        "emailAddress": {
            "$id": "#/properties/emailAddress",
            "type": "string",
            "title": "The emailAddress schema",
            "description": "The user email address.",
            "default": "",
            "examples": [
                "nfury@shield.gov"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "The user password.",
            "default": "",
            "examples": [
                "superSecretPassword"
            ]
        },
        "createdDate": {
            "$id": "#/properties/createdDate",
            "type": "string",
            "title": "The createdDate schema",
            "description": "The date a specified User entity was created.",
            "default": "",
            "examples": [
                "2021-02-24T17:45:36.230152"
            ]
        },
        "lastModified": {
            "$id": "#/properties/lastModified",
            "type": "string",
            "title": "The lastModified schema",
            "description": "The date a specified User entity was last modified.",
            "default": "",
            "examples": [
                "2021-01-24T17:45:36.230152"
            ]
        }
    },
    "additionalProperties": false
}